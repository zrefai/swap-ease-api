{
  "name": "swap-ease-api",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node build/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.3",
    "@types/lodash.chunk": "^4.2.7",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "api": "^4.5.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "https": "^1.0.0",
    "jest": "^29.0.2",
    "lodash.chunk": "^4.2.0",
    "mongodb": "^4.9.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.1.13",
    "tsoa": "^4.1.1"
  }
}
